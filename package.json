    "jsonwebtoken": "9.0.2",
    "zod": "4.1.12"
  },
  "devDependencies": {
    "@types/bcrypt": "6.0.0",
    "@types/cookie-parser": "1.4.10",
    "@types/cors": "2.8.19",
    "@types/express": "5.0.5",
    "@types/jsonwebtoken": "9.0.10",
    "@types/node": "24.10.0",
    "prisma": "6.19.0",
    "ts-node": "10.9.2",
    "tsx": "4.20.6",
    "typescript": "5.9.3"
